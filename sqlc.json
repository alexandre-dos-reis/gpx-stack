{
  "version": "2",
  "sql": [
    {
      "engine": "postgresql",
      "schema": "./database/migrations",
      "queries": "./database/query.sql",
      "gen": {
        "go": {
          "package": "repository",
          "out": "./database/repository",
          "sql_package": "pgx/v5",
          "emit_json_tags": true
        }
      }
    }
  ]
}
